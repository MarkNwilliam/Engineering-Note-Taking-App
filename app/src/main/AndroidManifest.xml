<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.engineeringtutorseries">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Edit"
            android:label="@string/title_activity_edit"
            android:parentActivityName=".WriteNotesActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.engineeringtutorseries.WriteNotesActivity" />
        </activity>
        <activity android:name=".QuestionDetailActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".QuestionActivity" />
        </activity>
        <activity android:name=".QuestionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".VideoDetailActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".VideoActivity" />
        </activity>
        <activity android:name=".VideoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".LectureNotesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TitleActivity" />
        </activity>
        <activity android:name=".TitleActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".AddNoteActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".WriteNotesActivity" />
        </activity>
        <activity android:name=".WriteNotesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".Intro2Activity" />
        <activity android:name=".introActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>